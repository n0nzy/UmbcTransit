<resources>

    <!--
         Used in Action Bar, and in AndroidManifest to tell the device the name of this app.
         It's to keep this short, so your launcher icon isn't displayed with "The greatest Wea"
         or something similar.
    -->
    <string name="app_name">UMBC Transit Guide</string>


    <string name="hello_world">Hello world!</string>

    <string name="fav_stops_fragment">Your Favorite Stops</string>
    <string name="search_stops_fragment">Enter Stop Id</string>

    <string name="stop_name"> Arbutus </string>

    <string name="title_activity_route_details">RouteDetailsActivity</string>

    <string name="title_activity_route">Route Details Activity</string>

    <string name="title_activity_filter_list">Filter ListView Activity</string>

    <!--
         By convention, "action" denotes that this String will be used as the label for an Action,
         typically from the action bar.  The ActionBar is limited real estate, so shorter is better.
    -->
    <string name="action_settings">Settings</string>
    <string name="action_route_list">F-List</string>

</resources>